pipeline {
    agent any

   triggers { pollSCM('* * * * *') }
    stages {

        stage('vcs') {

            steps {
                git url: 'https://github.com/Praveen-711/shopizer.git'

            }
         }
       stage('build'){
        steps{
             sh 'mvn package'
            }
          }

     stage('artifacts'){
   steps{

        archiveArtifacts artifacts: '**/target/*.jar'
}
}
   stage('junits')
{
steps{
           junit '**/surefire-reports/*.xml'}
           }
} 
}
